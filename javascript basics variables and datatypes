/* objects in JS - properties/ characteristics */
const person = {
    name: 'Shourya kapoor',
    age: 20,
    gender: 'male',
    isCoder: 'true',
}
console.log(person);

person['age'] = person['age'] + 1 ;

console.log(person['age']);
